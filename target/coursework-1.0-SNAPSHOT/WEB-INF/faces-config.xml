<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/main.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>film_view</from-outcome>
            <to-view-id>/film_view.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>registration</from-outcome>
            <to-view-id>/registration.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addFilm</from-outcome>
            <to-view-id>/addFilm.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>filmsNotFound</from-outcome>
            <to-view-id>/filmsNotFound.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editFilm</from-outcome>
            <to-view-id>/editFilm.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <to-view-id>/search_full.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/film_view.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>registration</from-outcome>
            <to-view-id>/registration.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addFilm</from-outcome>
            <to-view-id>/addFilm.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>form</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>filmsNotFound</from-outcome>
            <to-view-id>/filmsNotFound.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>search_full</from-outcome>
            <to-view-id>/search_full.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/search_full.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>filmsNotFound</from-outcome>
            <to-view-id>/filmsNotFound.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/filmsNotFound.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>search_full</from-outcome>
            <to-view-id>/search_full.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>filmsNotFound</from-outcome>
            <to-view-id>/filmsNotFound.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/editFilm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/addFilm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/registration.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>